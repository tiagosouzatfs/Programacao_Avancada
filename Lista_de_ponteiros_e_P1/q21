q21.

#include<stdio.h>
#include<stdlib.h>
#include<time.h>
#include <gc.h>
void GC(){
    for (int i = 0; i < 10000000; ++i){
        int *p = (int *) GC_MALLOC(sizeof(int));
    }
}
void Mall(){
    for (int i = 0; i < 10000000; ++i){
        int *p = (int *) malloc(sizeof(int)); // aloca
        free(p); // e jÃ¡ libera
    }
}
int main()
{
    clock_t t;
    t = clock();
    GC();
    t = clock() - t;
    printf("Tempo gasto com o libGC: %ldms.\n",
           (t*1000)/CLOCKS_PER_SEC);
    t = clock();
    Mall();
    t = clock() - t;
    printf("Tempo gasto com o Malloc: %ldms.\n",
           (t*1000)/CLOCKS_PER_SEC);
    
    return 0;
}
